{
    "name": "per3evere/preq",
    "description": "Microservices IPC command factory for Laravel && Lumen",
    "license": "MIT",
    "keywords": [
        "microservices", "ipc", "laravel"
    ],
    "authors": [
        {
            "name": "Persevere Von",
            "email": "wu543065657@163.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "~5.1",
        "guzzlehttp/promises": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "illuminate/cache": "~5.6.0",
        "illuminate/filesystem": "~5.6.0"
    },
    "suggest": {
        "laravel/framework": "To test the Laravel bindings",
        "laravel/lumen-framework": "To test the lumen bindings"
    },
    "autoload": {
        "psr-4": {
            "Per3evere\\Preq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Per3evere\\Preq\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Per3evere\\Preq\\PreqServiceProvider"
            ]
        }
    }
}
